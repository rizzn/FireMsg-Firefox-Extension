{
	"manifest_version":2,
	"name":"FIREMSG - Burn After Reading",
	"description":"Create burn-after-read messages via firemsg.cc",
	"version":"1.0",
	"browser_action":{
		"default_popup":"popup.html",
		"default_icon":"icons/logo48.png"
	},
	"icons":{
		"16":"icons/logo16.png",
		"48":"icons/logo48.png",
		"128":"icons/logo128.png"
	},
	"web_accessible_resources":[
		"fonts/*.ttf"
	],
	"permissions":["clipboardWrite"]
}